<p id="notice"><%= notice %></p>

<h1>Jobseekers</h1>

<table>
  <thead>
    <tr>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Contact</th>
      <th>Email</th>
      <th>Address</th>
      <th>Job title</th>
      <th>Password</th>
      <th>Password confirmation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jobseekers.each do |jobseeker| %>
      <tr>
        <td><%= jobseeker.firstname %></td>
        <td><%= jobseeker.lastname %></td>
        <td><%= jobseeker.contact %></td>
        <td><%= jobseeker.email %></td>
        <td><%= jobseeker.address %></td>
        <td><%= jobseeker.job_title %></td>
        <td><%= jobseeker.password %></td>
        <td><%= jobseeker.password_confirmation %></td>
        <td><%= link_to 'Show', jobseeker %></td>
        <td><%= link_to 'Edit', edit_jobseeker_path(jobseeker) %></td>
        <td><%= link_to 'Destroy', jobseeker, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jobseeker', new_jobseeker_path %>
